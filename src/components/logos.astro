---
// @ts-ignore
import { Icon } from "astro-icon";
---

<div class="mt-24">
  <h2 class="text-center text-slate-500">Empowering the latest technologies</h2>
  <div class="flex gap-8 md:gap-20 items-center justify-center mt-10 flex-wrap">
    <Icon class="h-8 md:h-12" name="simple-icons:python" />
    <Icon class="h-8 md:h-12" name="simple-icons:tensorflow" />
    <Icon class="h-8 md:h-14" name="simple-icons:typescript" />
    <Icon class="h-8 md:h-16" name="simple-icons:pytorch" />
    <Icon class="h-8 md:h-12" name="simple-icons:anaconda" />
    <Icon class="h-8 md:h-12" name="simple-icons:javascript" />
  </div>
</div>
